{
  "no-update-notifier": true,
  "ignore": [
    "actions/*",
    ".next/*",
    ".vscode/*",
    "components/*",
    "pages/*",
    "reducers/*",
    "static/*",
    "store/*"
  ],
  "execMap": {
    "js":
      "node --use-strict --harmony --optimize_for_size --max_old_space_size=460 --gc_interval=100"
  }
}
